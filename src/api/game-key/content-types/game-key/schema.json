{
  "kind": "collectionType",
  "collectionName": "game_keys",
  "info": {
    "singularName": "game-key",
    "pluralName": "game-keys",
    "displayName": "GameKey"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "product": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::product.product",
      "inversedBy": "gamekeys"
    },
    "key": {
      "type": "string",
      "required": true
    },
    "statuss": {
      "type": "enumeration",
      "enum": [
        "Available",
        " Sold ",
        "Refunded ",
        "Replaced"
      ]
    },
    "usedAt": {
      "type": "datetime"
    },
    "orderId": {
      "type": "string"
    },
    "order": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::order.order",
      "inversedBy": "gamekeys"
    }
  }
}
